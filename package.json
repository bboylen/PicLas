{
  "name": "@bboylen/piclash",
  "private": true,
  "version": "1.0.0",
  "author": "Brian Boylen",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "workspaces/*"
    ]
  },
  "scripts": {
    "start:dev": "yarn start:dev:client && yarn start:dev:server",
    "start:dev:client": "yarn workspace @piclash/client start:dev",
    "start:dev:server": "yarn workspace @piclash/server start:dev",
    "start:prod:client": "yarn workspace @piclash/client start:prod",
    "start:prod:server": "yarn workspace @piclash/server start:prod"
  }
}
